{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"lib": ["es2019", "DOM"],

		"allowJs": true,
		"noImplicitAny": false,
		"removeComments": false,
		"noUnusedLocals": false,
		"noImplicitThis": true,
		"inlineSourceMap": false,
		"sourceMap": true,
		"outDir": "out",
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"noUnusedParameters": false,
		"esModuleInterop": true
	},
	"include": [
		"src/**/*",
	],
	"exclude": [
		"out/**/*"
	]
}
